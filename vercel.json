{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": "astro",
  "headers": [
    {
      "source": "/giscus/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://giscus.app" }
      ]
    }
  ]
}
